{
  "name": "mark-to-jsonml",
  "version": "0.4.1",
  "description": "Parse markdown into JsonML",
  "keywords": [
    "markdown",
    "text processing",
    "jsonml"
  ],
  "author": {
    "name": "ljh131",
    "email": "ljh131@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ljh131/mark-to-jsonml.git"
  },
  "main": "./dist/parser.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src/*.js",
    "transpile": "babel src --plugins transform-remove-console --out-dir dist",
    "build": "npm run lint && npm run transpile",
    "pretest": "npm run build",
    "test": "mocha",
    "sandbox": "babel src --out-dir sandbox && node",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "autobind-decorator": "^2.2.1",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "mocha": "^3.5.3"
  }
}
