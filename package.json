{
  "name": "mark-to-jsonml",
  "version": "0.3.1",
  "description": "Parse markdown into JsonML",
  "keywords": [
    "markdown",
    "text processing",
    "jsonml"
  ],
  "author": {
    "name": "ljh131",
    "email": "ljh131@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ljh131/mark-to-jsonml.git"
  },
  "main": "./dist/parser.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src/*.js",
    "transpile": "babel src --presets babel-preset-es2015 --plugins transform-remove-console --out-dir dist",
    "build": "npm run lint && npm run transpile",
    "pretest": "npm run build",
    "test": "mocha",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "mocha": "^3.5.3"
  }
}
